package com.concurrent;

import java.util.concurrent.ConcurrentHashMap;

public class A {
	public static void main(String[] args) {
		ConcurrentHashMap m = new ConcurrentHashMap();
		m.put(101, "A");
		m.put(102, "B");
		m.putIfAbsent(103, "C");
		m.putIfAbsent(101, "D");
		m.remove(101, "D");
		m.replace(102, "M", "E");
		System.out.println(m);
		
	}

}
